{
    "opts": {
        "private": true,
        "recurse": true,
        "lenient": true,
        "destination": "./docs/mapboxgl",
        "template": "./node_modules/ink-docstrap/template"
    },
    "disqus": "",
    "tags": {
        "allowUnknownTags": true,
        "linkdoc-mapboxgl": "https://www.mapbox.com/mapbox-gl-js/api/"
    },
    "i18n": {
        "defaultLang": "en",
        "possibleLang": ["en"],
        "dbFilePath": "./doc/jsdocs/mapboxgl"
    },
    "source": {
        "includePattern": ".+\\.js?$",
        "include": [
            "src/common/", "src/mapboxgl/"
        ],
        "exclude": [
            "src/common/network/FetchRequest.js",
            "src/mapboxgl/overlay/mapv/"
        ]
    },
    "plugins": ["plugins/markdown","../plugins/factoryFunction","../plugins/linkdoc"],
    "templates": {
        "cleverLinks": true,
        "monospaceLinks": true,
        "dateFormat": "ddd MMM Do YYYY",
        "outputSourceFiles": true,
        "outputSourcePath": true,
        "systemName": "中地数码 WebClient",
        "footer": "",
        "copyright": "中地数码 WebClient && DocStrap Copyright © 2012-2015 The contributors to the JSDoc3 and DocStrap projects.",
        "navType": "vertical",
        "theme": "paper",
        "linenums": true,
        "collapseSymbols": false,
        "inverseNav": true
    }
}
